import java.util.Scanner;

public class Calc {
    public static void main(String[] args) {
      Scanner kb = new Scanner (System.in);
      int a ;
      System.out.print("Please input a number - ");
      a= kb.nextInt();
                int n = 2;
        int fac = 1;
        while (n <= a) {
            fac = fac * n;  
            n++;
        }
        System.out.println("Answer = " + fac);
        
        // Yout code stops here!
    }
}
