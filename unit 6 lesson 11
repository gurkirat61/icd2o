// any import statements go here!
import java.util.Scanner;

public class Emoji {
    public static void main(String[] args) {
      // Your code starts here!
      long smiley = 0x1f600;
      String uChar = new String(Character.toChars((int) smiley));
      System.out.printf("%4h\t%s\n", smiley, uChar);
      for (long i= smiley; i<smiley + 80; i++){
        uChar = new String(Character.toChars((int) i));
        System.out.printf("%6h\t%s  ", i, uChar);
        if ((i-smiley +1)%8==0){ System.out.println();
      }
  
      // Your code stops here!
    }
}
}
